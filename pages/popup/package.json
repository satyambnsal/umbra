{
  "name": "@extension/popup",
  "version": "0.3.2",
  "description": "chrome extension - popup",
  "private": true,
  "sideEffects": true,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "clean:node_modules": "pnpx rimraf node_modules",
    "clean:turbo": "pnpx rimraf .turbo",
    "clean": "pnpm clean:node_modules && pnpm clean:turbo",
    "build": "node --max-old-space-size=8192 ../../node_modules/vite/bin/vite.js build",
    "dev": "cross-env __DEV__=true vite build --mode development",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier . --write --ignore-path ../../.prettierignore",
    "type-check": "tsc --noEmit",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@aztec/accounts": "0.51.0",
    "@aztec/aztec.js": "0.51.0",
    "@aztec/circuits.js": "0.51.0",
    "@aztec/kv-store": "^0.52.0",
    "@aztec/noir-contracts.js": "0.51.0",
    "@extension/content-runtime-script": "workspace:*",
    "@extension/shared": "workspace:*",
    "@extension/storage": "workspace:*",
    "@headlessui/react": "2.1.2",
    "chalk": "^5.3.0",
    "clsx": "2.1.1",
    "dayjs": "1.11.12",
    "jotai": "^2.9.3",
    "lucide-react": "^0.439.0",
    "next-themes": "0.3.0",
    "react-router-dom": "6.25.1",
    "sonner": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.3.0",
    "@types/react": "^18.3.4",
    "patch-package": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "node-stdlib-browser": "^1.2.0",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vite-plugin-resolve": "^2.5.2",
    "@extension/tailwindcss-config": "workspace:*",
    "@extension/tsconfig": "workspace:*",
    "@extension/vite-config": "workspace:*",
    "cross-env": "^7.0.3",
    "daisyui": "^4.12.10",
    "postcss-load-config": "^6.0.1",
    "react-error-boundary": "4.0.13",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "1.0.7",
    "vite-plugin-svgr": "4.2.0"
  }
}
